{
  "name": "@goi/shared",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./user": {
      "types": "./dist/adapter/app/user/index.d.ts",
      "default": "./dist/adapter/app/user/index.js"
    },
    "./contracts/app/user": {
      "types": "./dist/contracts/app/user/index.d.ts",
      "default": "./dist/contracts/app/user/index.js"
    },
    "./domain/app/user": {
      "types": "./dist/domain/app/user/index.d.ts",
      "default": "./dist/domain/app/user/index.js"
    },
    "./postgresql": {
      "types": "./dist/infra/db/postgresql/index.d.ts",
      "default": "./dist/infra/db/postgresql/index.js"
    }
  },
  "scripts": {
    "dev": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "format": "prettier . --write",
    "lint": "biome lint",
    "check": "biome check",
    "db:gen": "drizzle-kit generate",
    "db:push": "drizzle-kit migrate",
    "db:update": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "drizzle-orm": "^0.45.1",
    "nestjs-zod": "^5.1.1",
    "pg": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8"
  }
}
