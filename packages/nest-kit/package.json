{
  "name": "@goi/nest-kit",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "default": "./dist/security/index.js"
    },
    "./transport": {
