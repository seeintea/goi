{
  "name": "goi",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "nx": "nx",
    "prepare": "npm run build:libs",
    "dev": "nx run-many -t dev -p admin admin-api app app-api",
    "dev:all": "npm run build:libs && concurrently -k --raw \"npm:dev:libs\" \"npm:dev\"",
    "dev:admin-group": "nx run-many -t dev -p admin admin-api",
    "dev:app-group": "nx run-many -t dev -p app app-api",
    "dev:admin-all": "npm run build:libs && concurrently -k --raw \"npm:dev:libs\" \"npm:dev:admin-group\"",
    "dev:app-all": "npm run build:libs && concurrently -k --raw \"npm:dev:libs\" \"npm:dev:app-group\"",
    "dev:admin": "nx run admin:dev",
    "dev:admin-api": "nx run admin-api:dev",
    "dev:app": "nx run app:dev",
    "dev:app-api": "nx run app-api:dev",
    "dev:libs": "nx run-many -t build -p nest-kit infra contracts utils utils-node utils-web --watch",
    "build:libs": "nx run-many -t build -p nest-kit infra contracts utils utils-node utils-web",
    "build": "nx run-many -t build",
    "fmt": "prettier . --check",
    "fmt:write": "prettier . --write",
    "lint": "nx run-many -t lint",
    "lint:write": "pnpm -r run lint:write",
    "typecheck": "nx run-many -t typecheck",
    "clean": "nx reset && npm run clean:dist",
    "clean:dist": "node scripts/clean-dist.mjs",
    "clean:modules": "node scripts/clean-node-modules.mjs"
  },
  "keywords": [],
  "author": "leviegu <leviegu@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@nestjs/cli": "^11.0.16",
    "@prettier/plugin-oxc": "^0.1.3",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.0",
    "concurrently": "^9.2.1",
    "nx": "^22.4.5",
    "prettier": "3.8.1",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.2.4",
      "react-dom": "^19.2.4",
      "typescript": "^5.9.3",
      "@nestjs/common": "^11.1.12",
      "@nestjs/core": "^11.1.12",
      "@nestjs/platform-express": "^11.1.12",
      "rxjs": "^7.8.2",
      "drizzle-orm": "^0.45.1"
    }
  }
}
